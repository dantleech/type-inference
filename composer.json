{
    "name": "dantleech/type-inference",
    "description": "Infer types in file offsets",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Microsoft/tolerant-php-parser"
        },
        {
            "type": "vcs",
            "url": "https://github.com/dantleech/worse-reflection"
        },
        {
            "type": "vcs",
            "url": "https://github.com/dantleech/class-to-file"
        }
    ],
    "require": {
        "microsoft/tolerant-php-parser": "dev-master",
        "dantleech/class-to-file": "dev-master",
        "roave/better-reflection": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "DTL\\TypeInference\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DTL\\TypeInference\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
